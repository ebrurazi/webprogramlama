@page
@model MyAspNetApp.Pages.UserDashboardModel

@{
    ViewData["Title"] = "User Dashboard";
}

<h2>User Dashboard</h2>
<p>Kullanıcı sayfasına hoş geldiniz!</p>

<h3>Ürün Listesi</h3>
<table class="table">
    <thead>
        <tr>
            <th>Ürün Adı</th>
            <th>Fiyat</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model.Products)
        {
            <tr>
                <td>@product.Name</td>
                <td>@product.Price</td>
            </tr>
        }
    </tbody>
</table>
