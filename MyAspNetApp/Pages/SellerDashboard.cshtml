@page
@model MyAspNetApp.Pages.SellerDashboardModel

@{
    ViewData["Title"] = "Satıcı Sayfası";
}

<h2>@ViewData["Title"]</h2>

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="productName">Ürün Adı</label>
        <input type="text" class="form-control" id="productName" name="productName" required>
    </div>
    <div class="form-group">
        <label for="productPrice">Ürün Fiyatı</label>
        <input type="number" class="form-control" id="productPrice" name="productPrice" required>
    </div>
    <div class="form-group">
        <label for="productImage">Ürün Resmi</label>
        <input type="file" class="form-control" id="productImage" name="productImage" required>
    </div>
    <button type="submit" class="btn btn-primary">Ürünü Kaydet</button>
</form>
